<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <stroke android:color="#3d3e3d" android:width="2dp"></stroke>
            <solid android:color="#050707"></solid>
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <stroke android:color="#ff292a29" android:width="2dp"></stroke>
            <solid android:color="#050707"></solid>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#050707"></solid>
        </shape>
    </item>
</selector>